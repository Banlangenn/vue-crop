{
  "name": "vue-cropblg",
  "main": "./lib/vue-cropblg.umd.min",
  "description": "vue 图片裁剪插件",
  "version": "1.0.16",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name vue-cropblg --dest lib packages/index.js"
  },
  "author": "https://github.com/Banlangenn",
  "license": "ISC",
  "keywords": [
    "vue",
    "vue-crop",
    "crop"
  ],
  "repository": "https://github.com/Banlangenn/vue-crop",
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-eslint": "^3.4.1",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.12.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.7",
    "vue-template-compiler": "^2.6.7"
  },
  "pre-commit": [
    "lint"
  ],
  "rules": {
    "no-console": 0,
    "indent": [
      "error",
      4
    ],
    "no-useless-escape": 0,
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "always"
    ],
    "no-cond-assign": [
      "error",
      "always"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
