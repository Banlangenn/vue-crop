{
  "name": "vue-progresschart",
  "main": "./lib/vue-progresschart.umd.min",
  "description": "一个vue 简单图表插件,目前支持:弧形进度(仪表盘)/线进度/饼图",
  "version": "1.1.4",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name vue-progresschart --dest lib packages/index.js"
  },
  "author": "https://github.com/Banlangenn",
  "license": "ISC",
  "keywords": [
    "vue",
    "vue-chart",
    "vue-progress",
    "progress"
  ],
  "repository": "https://github.com/Banlangenn/vue-progresschart",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21",
    "vue": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
